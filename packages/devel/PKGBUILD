pkgname=sock_connect_devel
pkgver=0.1
pkgrel=1
pkgdesc="Prototype shared library of Socket Connector with debug logging"
conflicts=(sock_connect)
arch=('i686' 'x86_64')
url="https://github.com/DGolgovsky/sock_connect"
license=('MIT')
groups=('')
options=('!strip' '!emptydirs')
depends=(gcc cmake doxygen git)
source_i686=("sock_connect-0.1-Linux.deb")
source_x86_64=("sock_connect-0.1-Linux.deb")

package() {
    # Extract package data
	tar xzf data.tar.gz -C "${pkgdir}"
}
