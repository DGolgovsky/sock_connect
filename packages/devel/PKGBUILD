pkgname=sock_connect_devel
pkgver=0.0.1
pkgrel=1
pkgdesc="Prototype shared library of Socket Connector. Devel version."

arch=('i686' 'x86_64')
url="https://github.com/DGolgovsky/sock_connect"
license=('MIT')
options=('!strip' '!emptydirs')
depends=(glibc cmake gcc)

source_i686=("sock_connect_devel-$pkgver-Linux.deb")
source_x86_64=("sock_connect_devel-$pkgver-Linux.deb")

package() {

	# Extract package data
	tar xzf data.tar.gz -C "${pkgdir}"

}
